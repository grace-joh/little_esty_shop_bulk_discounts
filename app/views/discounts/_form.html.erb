  <%= form_with model: model, url: url, method: method, local: true do |f| %>
    <%= f.label :percent_integer, "Percent Discount" %>
    <p><%= f.number_field :percent_integer, in: 1..100 %>%</p>
    <%= f.label :min_quantity, "Minimum Quantity" %>
    <p><%= f.number_field :min_quantity %></p>
    <%= f.submit 'Create' %>
  <% end %>